@model MvcIng.Models.LogInModel
@using Buisiness;
@{
    ViewBag.Title = "Login";
    Layout = null;
}
<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

@Styles.Render("~/bundles/Style")
<html>
<body>
    <div class="fluid">
        <form class="login-container" method="post" action="@Url.Action("LogIn", "LogIn")">

            <div class="logo-container login-logo-container">
                <img src="data:image/png;base64,">
            </div>
            <div class="login-form-container table-container">
                <div class="login-subcontainer">
                    <div class="input-container">
                        <span class="fa fa-user"></span>
                        <input type="text" name="username" placeholder="Enter your User Name" value="@Model.username" />
                        
                    </div>
                    <div class="input-container">
                        <span class="fa fa-key"></span>
                        <input type="password" name="password" value="@Model.password" placeholder="Enter Password" />
                       
                    </div>
                    <input type="checkbox" name="checkbox" id="checkbox">
                    <label for="checkbox">Remember me?</label>
                    <a href="#">Forgot your Login?</a>
                    <input type="submit" value="LOGIN" class="bottom-button" />
                </div>
            </div>
            @if(ViewBag.BadLogin!=null && ViewBag.BadLogin)
            { 
            <div class="error bottom-button">
                <p>Wrong User Name and/or Password!</p>
            </div>
            }
        </form>



    </div>


    <script type="text/javascript" src="~/Scripts/jquery-2.1.4.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-ui.js"></script>
    <script type="text/javascript" src="~/Scripts/main.js"></script>

</body>
</html>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


